//prime number
// Online Java Compiler
// Use this editor to write, compile and run your Java code online

class Main {
    public static void main(String[] args) {
        prime(2);
    }
    static void prime(int a)
    {int count=0;
        for(int i=1;i<=a;i++)
        {
            if(a%i==0)
            {
                count =count +1 ;
            }
        }
    if(count==2)
    {
        System.out.println("prime");
    }
     else
    {
        System.out.println(" Not prime");
    }   
    }
}


